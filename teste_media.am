/*Programa que calcula a m√©dia dos valores de um vetor*/
int vetor[10];

int main()
{
	vetor[0] = 5;
	vetor[1] = 90;
	vetor[2] = 8;
	vetor[3] = 13;
	vetor[4] = 42;
	vetor[5] = 33;
	vetor[6] = 11;
	vetor[7] = 1;
	vetor[8] = 56;
	vetor[9] = 9;

	int i;
	int total;
	int average;

	i = 0;
	total = 0;
	average = 0;

	while (i<10) do
	{
		total = total + vetor[i];
	};

	average = total / 10;
	
	return average;
}